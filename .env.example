# 專案名稱（用於區分不同環境）
PROJECT_NAME=my_dev_env

# Nginx設定
NGINX_HTTP_PORT=80
NGINX_HTTPS_PORT=443

# PHP 7.3設定
PHP73_MEMORY_LIMIT=512M
PHP73_MAX_EXECUTION_TIME=60
PHP73_UPLOAD_MAX_FILESIZE=20M
PHP73_POST_MAX_SIZE=20M

# PHP 7.4設定
PHP74_MEMORY_LIMIT=512M
PHP74_MAX_EXECUTION_TIME=60
PHP74_UPLOAD_MAX_FILESIZE=20M
PHP74_POST_MAX_SIZE=20M

# PHP 8.0設定
PHP80_MEMORY_LIMIT=512M
PHP80_MAX_EXECUTION_TIME=60
PHP80_UPLOAD_MAX_FILESIZE=20M
PHP80_POST_MAX_SIZE=20M

# PHP 8.1設定
PHP81_MEMORY_LIMIT=512M
PHP81_MAX_EXECUTION_TIME=60
PHP81_UPLOAD_MAX_FILESIZE=20M
PHP81_POST_MAX_SIZE=20M

# PHP 8.3設定
PHP83_MEMORY_LIMIT=512M
PHP83_MAX_EXECUTION_TIME=60
PHP83_UPLOAD_MAX_FILESIZE=20M
PHP83_POST_MAX_SIZE=20M

# MySQL設定
MYSQL_VERSION=8.0
MYSQL_PORT=3306
MYSQL_ROOT_PASSWORD=root
MYSQL_DATABASE=laravel
MYSQL_USER=developer
MYSQL_PASSWORD=secret

# Redis設定
REDIS_VERSION=6.2
REDIS_PORT=6379
REDIS_PASSWORD=null

# 工作區設定
NODE_VERSION=16
INSTALL_YARN=true
INSTALL_PM2=true
WORKSPACE_SSH_PORT=2222
WORKSPACE_NODE_PORT=3000
WORKSPACE_NODE_PORT_RANGE_START=3001
WORKSPACE_NODE_PORT_RANGE_END=3010